{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.6.0"
  },
  "devDependencies": {
    "canonical-json": "0.0.4",
    "chexo": "1.0.7",
    "handlebars": "4.1.1",
    "hexo": "3.8.0",
    "hexo-prism-plus": "1.1.0",
    "hexo-renderer-ejs": "0.3.1",
    "hexo-renderer-less": "0.2.0",
    "hexo-renderer-marked": "0.3.2",
    "hexo-server": "0.3.3",
    "hexo-versioned-netlify-redirects": "1.1.0",
    "jsdoc": "3.5.5",
    "meteor-hexo-config": "1.0.9",
    "meteor-theme-hexo": "2.0.1",
    "showdown": "1.9.0",
    "underscore": "1.9.1"
  },
  "scripts": {
    "build": "jsdoc/jsdoc.sh && chexo meteor-hexo-config -- generate",
    "clean": "hexo clean; rm data/data.js data/names.json",
